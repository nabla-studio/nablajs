{
	"extends": ".releaserc.base.json",
	"pkgRoot": "dist/packages/rn-bip85",
	"tagFormat": "rn-bip85-v${version}",
	"commitPaths": ["packages/rn-bip85/*"],
	"plugins": [
		"@semantic-release/commit-analyzer",
		"@semantic-release/release-notes-generator",
		[
			"@semantic-release/changelog",
			{
				"changelogFile": "packages/rn-bip85/CHANGELOG.md"
			}
		],
		[
			"@semantic-release/npm",
			{
				"pkgRoot": "dist/packages/rn-bip85"
			}
		],
		[
			"@semantic-release/git",
			{
				"assets": [
					"packages/rn-bip85/package.json",
					"packages/rn-bip85/CHANGELOG.md"
				],
				"message": "release(version): Release rn-bip85 ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
			}
		]
	]
}
