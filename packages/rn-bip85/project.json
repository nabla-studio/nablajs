{
	"name": "rn-bip85",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "packages/rn-bip85/src",
	"projectType": "library",
	"tags": [],
	"targets": {
		"build": {
			"executor": "@nx/rollup:rollup",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/packages/rn-bip85",
				"tsConfig": "packages/rn-bip85/tsconfig.lib.json",
				"project": "packages/rn-bip85/package.json",
				"entryFile": "packages/rn-bip85/src/index.ts",
				"external": ["react/jsx-runtime", "react-native"],
				"rollupConfig": "@nx/react/plugins/bundle-rollup",
				"assets": [
					{
						"glob": "packages/rn-bip85/README.md",
						"input": ".",
						"output": "."
					}
				]
			}
		},
		"lint": {
			"executor": "@nx/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["packages/rn-bip85/**/*.{ts,tsx,js,jsx}"]
			}
		},
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "packages/rn-bip85/jest.config.ts",
				"passWithNoTests": true
			}
		},
		"semantic-release": {
      "executor": "@theunderscorer/nx-semantic-release:semantic-release",
      "options": {
        "github": true,
        "changelog": true,
        "npm": true,
        "tagFormat": "rn-bip85-v${VERSION}",
        "outputPath": "dist/packages/rn-bip85"
      }
    }
	}
}
